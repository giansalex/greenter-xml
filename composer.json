    "keywords": ["greenter", "sunat", "xml", "facturacion electronica"],
    "license": "MIT",
    "authors": [
        {
            "name": "Giancarlos Salas",
            "email": "giansalex@gmail.com"
        }
    ],
    "homepage": "https://github.com/thegreenter/xml",
